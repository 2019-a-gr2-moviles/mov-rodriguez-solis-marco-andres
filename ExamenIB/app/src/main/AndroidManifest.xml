<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.examenib">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".CrearMedicamentosActivity"
                android:launchMode="singleTask">
        </activity>
        <activity android:name=".CrudHijosActivity"
                android:launchMode="singleTask">
        </activity>
        <activity android:name=".ListViewHijosActivity"
                android:launchMode="singleTask">
        </activity>
        <activity android:name=".CrudPapasActivity"
                android:launchMode="singleTask">
        </activity>
        <activity android:name=".CrearPacientesActivity"
                android:launchMode="singleTask">
        </activity>
        <activity android:name=".ListViewPapasActivity"
                android:launchMode="singleTask">
        </activity>
        <activity android:name=".MainPapasActivity"
                android:launchMode="singleTask">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>